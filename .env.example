# Supabase Configuration Template
# Copy this file to .env and replace the placeholder values with your actual credentials

# Your Supabase project URL (e.g., https://your-project-id.supabase.co)
SUPABASE_URL=https://your-project-id.supabase.co

# Your Supabase anon/public key (not the service role key!)
SUPABASE_ANON_KEY=your-anon-key-here

# Optional: Environment (development, staging, production)
ENVIRONMENT=development

# Optional: App version
APP_VERSION=1.0.0
